{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-unused-expression": true,
        "no-duplicate-variable": true,
        "no-duplicate-key": true,
        "no-unused-variable": true,
        "curly": true,
        "class-name": true,
        "no-console": false,
        "semicolon": true,
        "triple-equals": true,
        "quotemark": [
            true,
            "single"
        ]
    }
    ,
    "rulesDirectory": []
}